<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.MobileOrienteering" parent="android:Theme.Material.NoActionBar">
        <item name="android:windowBackground">#ece0c6</item>
        <item name="android:statusBarColor">#ece0c6</item>
        <item name="android:navigationBarColor">#ece0c6</item>
    </style>
    <style name="Theme.MobileOrienteering.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#ece0c6</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher</item>
        <item name="postSplashScreenTheme">
            @style/Theme.MobileOrienteering
        </item>
    </style>
</resources>
